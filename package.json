{
  "name": "featherwiki",
  "version": "0.0.1",
  "title": "Feather Wiki",
  "description": "It's like TiddlyWiki but as small as possible!",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir docs"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "outputFormat": "global"
    }
  },
  "dependencies": {
    "choo": "^7.1.0",
    "json-compress": "github:Alamantus/JSON-Compress",
    "pell": "^1.0.6"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "choo-devtools": "^3.0.4",
    "events": "^3.3.0",
    "minify-html-literals": "^1.3.5",
    "parcel": "^2.4.1",
    "parcel-transformer-inject-package-json-value": "file:parcel-transformer-inject-package-json-value",
    "parcel-transformer-minify-html-literals": "file:parcel-transformer-minify-html-literals"
  }
}
